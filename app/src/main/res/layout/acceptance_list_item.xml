<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:padding="4dp">

    <TextView
        android:id="@+id/txt_not_in_day"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/not_in_day"
        android:textAppearance="?android:attr/textAppearance"
        android:textColor="#000000"
        android:textStyle="bold" />

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/label_item_loc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/label_item_loc"
            android:textAppearance="?android:attr/textAppearance"
            android:textColor="#000000" />

        <TextView
            android:id="@+id/txt_item_loc"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:textAppearance="?android:attr/textAppearance"
            android:textColor="@color/blue" />

        <TextView
            android:id="@+id/label_item_loc_desc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/label_item_loc_desc"
            android:textAppearance="?android:attr/textAppearance"
            android:textColor="#000000" />

        <TextView
            android:id="@+id/txt_item_loc_desc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:textAppearance="?android:attr/textAppearance"
            android:textColor="@color/blue" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/label_receipt_no"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:text="@string/label_acc_receipt_no"
            android:textAppearance="?android:attr/textAppearance"
            android:textColor="#000000"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/txt_receipt_no"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:layout_weight="1"
            android:gravity="left"
            android:textAppearance="?android:attr/textAppearance"
            android:textColor="#000000" />
        <TextView
            android:id="@+id/label_trx_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="4dp"
            android:gravity="left"
            android:text="@string/label_trx_id"
            android:textAppearance="?android:attr/textAppearance"
            android:textColor="#000000"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/txt__trx_id"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:layout_weight="1"
            android:gravity="left"
            android:textAppearance="?android:attr/textAppearance"
            android:textColor="#000000" />


    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/label_control"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:text="@string/label_control"
            android:textAppearance="?android:attr/textAppearance"
            android:textColor="#000000"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/txt_control"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:layout_weight="1"
            android:gravity="left"
            android:text=""
            android:textAppearance="?android:attr/textAppearance"
            android:textColor="#000000" />

        <TextView
            android:id="@+id/label_part_number"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="4dp"
            android:gravity="left"
            android:text="@string/label_part_number"
            android:textAppearance="?android:attr/textAppearance"
            android:textColor="#000000"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/txt_part_number"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:layout_weight="3"
            android:gravity="left"
            android:text=""
            android:textAppearance="?android:attr/textAppearance"
            android:textColor="#000000" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/label_uncheck_qty"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:text="@string/label_uncheck_qty"
            android:textAppearance="?android:attr/textAppearance"
            android:textColor="#000000"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/txt_uncheck_qty"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:layout_weight="1"
            android:gravity="left"
            android:text=""
            android:textAppearance="?android:attr/textAppearance"
            android:textColor="#000000" />

        <TextView
            android:id="@+id/label_check_not_in"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="4dp"
            android:gravity="left"
            android:text="@string/label_check_not_in"
            android:textAppearance="?android:attr/textAppearance"
            android:textColor="#000000"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/txt_check_not_in_qty"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:layout_weight="1"
            android:gravity="left"
            android:text=""
            android:textAppearance="?android:attr/textAppearance"
            android:textColor="#000000" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/label_stock_in_qty"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:text="@string/label_stock_in_qty"
            android:textAppearance="?android:attr/textAppearance"
            android:textColor="#000000"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/txt_in_qty"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:layout_weight="1"
            android:gravity="left"
            android:text=""
            android:textAppearance="?android:attr/textAppearance"
            android:textColor="#000000" />

        <TextView
            android:id="@+id/label_not_in_qty"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="4dp"
            android:gravity="left"
            android:text="@string/label_not_in_qty"
            android:textAppearance="?android:attr/textAppearance"
            android:textColor="#000000"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/txt_not_in_qty"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:layout_weight="1"
            android:gravity="left"
            android:text=""
            android:textAppearance="?android:attr/textAppearance"
            android:textColor="#000000" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_xb"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/label_stock_in_xb_qty"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:text="@string/label_stock_in_xb_qty"
            android:textAppearance="?android:attr/textAppearance"
            android:textColor="#000000"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/txt_in_xb_qty"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:layout_weight="1"
            android:gravity="left"
            android:text=""
            android:textAppearance="?android:attr/textAppearance"
            android:textColor="#000000" />

        <TextView
            android:id="@+id/label_not_in_xb_qty"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="4dp"
            android:gravity="left"
            android:text="@string/label_not_in_xb_qty"
            android:textAppearance="?android:attr/textAppearance"
            android:textColor="#000000"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/txt_not_in_xb_qty"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:layout_weight="1"
            android:gravity="left"
            android:text=""
            android:textAppearance="?android:attr/textAppearance"
            android:textColor="#000000" />

    </LinearLayout>

</LinearLayout>