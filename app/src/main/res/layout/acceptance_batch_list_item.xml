<?xml version="1.0" encoding="utf-8"?>
<com.senao.warehouse.widget.CheckableLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:padding="4dp">

    <CheckBox
        android:id="@+id/checkbox"
        style="@style/CheckBoxAppTheme"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:clickable="false"
        android:focusable="false"
        android:padding="2dp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="4dp">

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/txt_not_in_day"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="7"
                android:text="@string/not_in_day"
                android:textAppearance="?android:attr/textAppearance"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/label_receipt_no"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="3"
                android:gravity="left"
                android:text="@string/label_acc_receipt_no"
                android:textAppearance="?android:attr/textAppearance"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/txt_receipt_no"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="3"
                android:gravity="left"
                android:textAppearance="?android:attr/textAppearance"
                android:textColor="#000000" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/label_control"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left"
                android:text="@string/label_control"
                android:textAppearance="?android:attr/textAppearance"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/txt_control"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left"
                android:text=""
                android:textAppearance="?android:attr/textAppearance"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/label_part_number"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left"
                android:text="@string/label_part_number"
                android:textAppearance="?android:attr/textAppearance"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/txt_part_number"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="3"
                android:gravity="left"
                android:text=""
                android:textAppearance="?android:attr/textAppearance"
                android:textColor="#000000" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/label_uncheck_qty"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:gravity="left"
                android:text="@string/label_uncheck_qty"
                android:textAppearance="?android:attr/textAppearance"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/txt_uncheck_qty"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="3"
                android:gravity="left"
                android:text=""
                android:textAppearance="?android:attr/textAppearance"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/label_check_not_in"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="3"
                android:gravity="left"
                android:text="@string/label_check_not_in"
                android:textAppearance="?android:attr/textAppearance"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/txt_check_not_in_qty"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="4"
                android:gravity="left"
                android:text=""
                android:textAppearance="?android:attr/textAppearance"
                android:textColor="#000000" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/label_stock_in_qty"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:gravity="left"
                android:text="@string/label_stock_in_qty"
                android:textAppearance="?android:attr/textAppearance"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/txt_in_qty"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="3"
                android:gravity="left"
                android:text=""
                android:textAppearance="?android:attr/textAppearance"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/label_not_in_qty"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:gravity="left"
                android:text="@string/label_not_in_qty"
                android:textAppearance="?android:attr/textAppearance"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/txt_not_in_qty"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="3"
                android:gravity="left"
                android:text=""
                android:textAppearance="?android:attr/textAppearance"
                android:textColor="#000000" />

        </LinearLayout>
    </LinearLayout>
</com.senao.warehouse.widget.CheckableLinearLayout>